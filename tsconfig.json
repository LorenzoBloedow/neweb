{
	"compilerOptions": {
		"target": "ES2022",
		"moduleResolution": "Bundler",
		"allowJs": true,
		"checkJs": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"importHelpers": true,
		"declaration": true,
		"declarationMap": true
	}
}
