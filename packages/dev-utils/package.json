	},
	"dependencies": {
		"@rollup/plugin-babel": "^6.0.3",
		"@rollup/plugin-terser": "^0.4.3",
		"neweb-local-utils": "workspace:^0.1.0",
		"rollup-plugin-ts": "^3.4.5",
		"tslib": "^2.6.2"
	}
}
