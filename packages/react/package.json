{
	"name": "@neweb/react",
	"version": "0.1.0",
	"description": "Next-gen component library redefining user expectations",
	"exports": {
		".": {
			"types": "./dist/types/hybrid/index.d.ts",
			"default": "./dist/hybrid/index.js"
		},
		"./browser": {
			"types": "./dist/types/browser/index.d.ts",
			"default": "./dist/browser/index.js"
		},
		"./node": {
			"types": "./dist/types/node/index.d.ts",
			"default": "./dist/node/index.js"
		}
	},
	"type": "module",
	"scripts": {
		"build": "rimraf dist && rollup --config rollup.config.ts --configPlugin rollup-plugin-ts",
		"dev": "rollup --config rollup.config.ts --configPlugin rollup-plugin-ts --watch"
	},
	"keywords": [
		"neweb",
		"components",
		"react"
	],
	"files": [
		"dist",
		"package.json"
	],
	"author": "Lorenzo Bloedow",
	"license": "MIT",
	"devDependencies": {
		"@babel/preset-react": "^7.22.5",
		"dev-utils": "workspace:^0.1.0",
		"rimraf": "^5.0.1",
		"rollup": "^3.28.1",
		"rollup-plugin-ts": "^3.4.5"
	},
	"dependencies": {
		"neweb-local-utils": "workspace:^0.1.0",
		"tslib": "^2.6.2"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	}
}
